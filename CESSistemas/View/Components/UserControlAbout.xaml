            Width="530" 
            Height="42" 
            Margin="39,231,0,0" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            materialDesign:HintAssist.Hint="Razão Social" 
            Style="{StaticResource MaterialDesignFloatingHintTextBox}" 
