            HorizontalAlignment="Left" 
            Width="530"
            Height="44" 
            Margin="39,278,0,0" 
            TextWrapping="Wrap" 
            VerticalAlignment="Top" 
            materialDesign:HintAssist.Hint="Endereço" 
